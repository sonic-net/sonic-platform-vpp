{
    "DEVICE_METADATA": {
        "localhost": {
            "mac": "{{ system_mac }}",
            "buffer_model": "traditional"
        }
    },
    "FLEX_COUNTER_TABLE": {
        "PORT": {
            "FLEX_COUNTER_STATUS": "enable"
        },
        "RIF": {
            "FLEX_COUNTER_STATUS": "enable"
        }
    },
{% set features = ["swss", "bgp", "teamd", "nat", "database", "lldp", "dhcp_relay", "macsec"] %}
    "FEATURE": {
{% for feature in features %}
        "{{ feature }}": {
            "state": "enabled"
        }{% if not loop.last %},{% endif %}
{% endfor %}
    }
}

